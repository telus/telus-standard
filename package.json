{
  "version": "0.0.0-development",
  "name": "@telus/telus-standard",
  "description": "Standard for TELUS Digital",
  "homepage": "https://github.com/telus/telus-standard",
  "repository": {
    "type": "git",
    "url": "https://github.com/telus/telus-standard.git"
  },
  "license": "UNLICENSED",
  "main": "index.js",
  "bin": {
    "telus-standard": "./bin/cmd.js"
  },
  "keywords": [
    "telus"
  ],
  "engines": {
    "node": ">=8"
  },
  "files": [
    "bin",
    "index.js",
    "options.js",
    "eslintrc.js"
  ],
  "bugs": {
    "url": "https://github.com/telus/telus-standard/issues"
  },
  "scripts": {
    "coverage": "opener coverage/lcov-report/index.html",
    "lint:audit": "audit-ci --config .audit-ci.json",
    "lint:deps": "npx updated --ignore=eslint,eslint-config-airbnb,editorconfig-checker",
    "lint:ec": "npx editorconfig-checker -v .",
    "lint:js": "./bin/cmd.js --verbose",
    "lint": "npx npm-run-all -p -c -l lint:*",
    "fix:js": "./bin/cmd.js --verbose --fix",
    "fix:deps": "npm run lint:deps -- --update",
    "show-rules": "eslint-index eslintrc.json",
    "show-rules-disabled": "eslint-index eslintrc.json --status omitted off",
    "show-summary": "eslint-index eslintrc.json --format table",
    "test": "./bin/cmd.js --verbose && tape test/*.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "standard-engine": "^12.0.0"
  },
  "devDependencies": {
    "@telus/semantic-release-config": "^1.3.9",
    "audit-ci": "^2.4.2",
    "cross-spawn": "^7.0.1",
    "editorconfig-checker": "^2.2.0",
    "eslint-index": "^1.5.0",
    "minimist": "^1.2.0",
    "mkdirp": "^1.0.3",
    "run-parallel-limit": "^1.0.5",
    "semantic-release": "^17.0.1",
    "standard-packages": "^3.8.2",
    "tape": "^4.13.0"
  },
  "telus-standard": {
    "ignore": [
      "/test"
    ]
  }
}
